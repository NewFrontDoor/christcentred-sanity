{
  "name": "christcentred-sanity",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "package.json",
  "author": "New Front Door <info@newfrontdoor.org>",
  "license": "UNLICENSED",
  "scripts": {
    "start": "sanity start",
    "test": "sanity check"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "@newfrontdoor/audio-player": "^0.1.2",
    "@newfrontdoor/bible": "0.2.3",
    "@sanity/base": "^2.0.9",
    "@sanity/color-input": "^2.0.9",
    "@sanity/components": "^2.0.9",
    "@sanity/core": "^2.0.9",
    "@sanity/dashboard": "^2.0.9",
    "@sanity/default-layout": "^2.0.9",
    "@sanity/default-login": "^2.0.9",
    "@sanity/desk-tool": "^2.0.9",
    "@sanity/google-maps-input": "^2.0.9",
    "@sanity/vision": "^2.0.9",
    "aws-sdk": "^2.814.0",
    "prop-types": "^15.7.2",
    "react": "^16.2",
    "react-dom": "^16.2",
    "react-dropzone": "^10.1.8",
    "react-icons": "^3.7.0",
    "react-spinners": "^0.6.1",
    "sanity-plugin-dashboard-widget-feed": "0.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "prettier": "^1.18.2",
    "xo": "^0.25.3"
  },
  "xo": {
    "extends": [
      "xo-react"
    ],
    "parser": "babel-eslint",
    "prettier": true,
    "space": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "off"
    },
    "overrides": [
      {
        "files": "**/__tests__/**/*.js",
        "rules": {
          "import/no-unassigned-import": [
            "error",
            {
              "allow": [
                "jest-dom/extend-expect"
              ]
            }
          ]
        }
      }
    ]
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
